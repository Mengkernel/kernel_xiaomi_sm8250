# Disable Unused Modules
CONFIG_MSM_11AD=n
CONFIG_MSM_RDBG=n
CONFIG_USB_GSPCA=n
CONFIG_DVB_MPQ=n
CONFIG_QCOM_LLCC_PERFMON=n

# Enable block device writeback throttling
CONFIG_BLK_WBT=y
CONFIG_BLK_WBT_SQ=y

# Enable QCA CLD WLAN
CONFIG_QCA_CLD_WLAN=y
CONFIG_QCA_CLD_WLAN_PROFILE="qca6390"

# Build DTBO.img
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# Disable module build
CONFIG_MODULES=n

# Disable Maxim fan controller
CONFIG_QTI_MAXIM_FAN_CONTROLLER=n

# Set SMP2P sleepstate time to 100ms
CONFIG_SMP2P_SLEEPSTATE_TIME=100

# Disable unused IO schedulers
CONFIG_IOSCHED_BFQ=n
CONFIG_BFQ_GROUP_IOSCHED=n
CONFIG_IOSCHED_DEADLINE=n
CONFIG_MQ_IOSCHED_DEADLINE=n
CONFIG_MQ_IOSCHED_KYBER=n

# Enable zRAM writeback
CONFIG_ZRAM_WRITEBACK=y

# Enable exFAT FS
CONFIG_EXFAT_FS=y

# Enable XFRM_MIGRATE
CONFIG_XFRM_MIGRATE=y

# Enable BLAKE2b support
CONFIG_CRYPTO_BLAKE2B=y

# Enable WIREGUARD VPN support
CONFIG_WIREGUARD=y

# Initialize locals with zeroes
CONFIG_INIT_STACK_ALL_ZERO=y

# Enable CONFIG_INIT_ON_ALLOC_DEFAULT_ON
CONFIG_INIT_ON_ALLOC_DEFAULT_ON=y

# unset unused DM_CRYPT Kconfig
CONFIG_DM_CRYPT=n

# Remove unused configs
CONFIG_RD_BZIP2=n
CONFIG_RD_LZMA=n
CONFIG_SYSFS_SYSCALL=n
CONFIG_CHR_DEV_SCH=n
CONFIG_VGA_ARB=n
CONFIG_QCOM_MINIDUMP=n

# Disable unused governors
CONFIG_CPU_FREQ_GOV_ONDEMAND=n
CONFIG_CPU_FREQ_GOV_CONSERVATIVE=n
CONFIG_CPU_FREQ_GOV_POWERSAVE=n
CONFIG_CPU_FREQ_GOV_USERSPACE=n

# Set CONFIG_DEFAULT_MMAP_MIN_ADDR to 32768
CONFIG_DEFAULT_MMAP_MIN_ADDR=32768

# Do not select CONFIG_FB for CONFIG_DRM
CONFIG_FB_ARMCLCD=n
CONFIG_BACKLIGHT_LCD_SUPPORT=y
CONFIG_BACKLIGHT_CLASS_DEVICE=y
CONFIG_LOGO=n

# Enable CONFIG_ION_SYSTEM_HEAP
CONFIG_ION_SYSTEM_HEAP=y

# Disable BALANCE_ANON_FILE_RECLAIM
CONFIG_BALANCE_ANON_FILE_RECLAIM=n

# Disable ION_POOL_AUTO_REFILL
CONFIG_ION_POOL_AUTO_REFILL=n

# Enable CONFIG_CRYPTO_CHACHA20POLY1305
CONFIG_CRYPTO_CHACHA20POLY1305=y

# Disable CONFIG_RSEQ
CONFIG_RSEQ=n

# Enable CONFIG_JUMP_LABEL
CONFIG_JUMP_LABEL=y

# Disable all kernel symbols generation
CONFIG_KALLSYMS_ALL=n

# Disable radio support
CONFIG_MEDIA_RADIO_SUPPORT=n
CONFIG_I2C_RTC6226_QCA=n

# Disable noop
CONFIG_IOSCHED_NOOP=n

# Set FQ Codel as default net scheduler
CONFIG_NET_SCH_FQ_CODEL=y
CONFIG_NET_SCH_DEFAULT=y
CONFIG_DEFAULT_FQ_CODEL=y

# Disable Controller Area Network (CAN) support
CONFIG_CAN=n

# Enable FTRACE
CONFIG_FTRACE=y

# Enable "TTL" target support
CONFIG_IP_NF_TARGET_TTL=y

# Disable KinecticsXR Nordic chip support
CONFIG_KINECTICS_XR_NORDIC=n

# Disable qcom crypto ICE
CONFIG_CRYPTO_DEV_QCOM_ICE=n

# Disable I210 driver for Ethernet Card
CONFIG_IGB=n

# Disable unused errata
CONFIG_ARM64_ERRATUM_826319=n
CONFIG_ARM64_ERRATUM_827319=n
CONFIG_ARM64_ERRATUM_824069=n
CONFIG_ARM64_ERRATUM_819472=n
CONFIG_ARM64_ERRATUM_832075=n
CONFIG_ARM64_ERRATUM_845719=n
CONFIG_ARM64_ERRATUM_843419=n
CONFIG_ARM64_ERRATUM_1188873=n
CONFIG_ARM64_ERRATUM_1463225=n
CONFIG_ARM64_ERRATUM_1542419=n
CONFIG_ARM64_ERRATUM_1742098=n
CONFIG_CAVIUM_ERRATUM_22375=n
CONFIG_CAVIUM_ERRATUM_23154=n
CONFIG_CAVIUM_ERRATUM_27456=n
CONFIG_CAVIUM_ERRATUM_30115=n
CONFIG_QCOM_FALKOR_ERRATUM_1003=n
CONFIG_QCOM_FALKOR_ERRATUM_1009=n
CONFIG_QCOM_QDF2400_ERRATUM_0065=n
CONFIG_SOCIONEXT_SYNQUACER_PREITS=n
CONFIG_HISILICON_ERRATUM_161600802=n
CONFIG_QCOM_FALKOR_ERRATUM_E1041=n

# Enable userspace CNTVCT_EL0 access for vDSO
CONFIG_ARM_ARCH_TIMER_VCT_ACCESS=y

# Don't compile out kernel headers
CONFIG_IKHEADERS=n

# Disable vm event counters
CONFIG_VM_EVENT_COUNTERS=n

# Disable Tablets input support
CONFIG_INPUT_TABLET=n
# Keep CONFIG_USB
CONFIG_USB=y

# Disable Digital TV support
CONFIG_MEDIA_DIGITAL_TV_SUPPORT=n

# Disable scsi logging
CONFIG_SCSI_CONSTANTS=n
CONFIG_SCSI_LOGGING=n

# Disable media ancillary drivers autoselecting support
CONFIG_MEDIA_SUBDRV_AUTOSELECT=n

# Disable ARMV8 deprecated instruction set emulation
CONFIG_ARMV8_DEPRECATED=n

# Disable Media USB Adapters
CONFIG_MEDIA_USB_SUPPORT=n

# Disable OKL4 Hypervisor guest support
CONFIG_OKL4_GUEST=n

# Disable unused WLAN driver
CONFIG_WLAN_VENDOR_ADMTEK=n
CONFIG_WLAN_VENDOR_ATH=n
CONFIG_WLAN_VENDOR_ATMEL=n
CONFIG_WLAN_VENDOR_BROADCOM=n
CONFIG_WLAN_VENDOR_CISCO=n
CONFIG_WLAN_VENDOR_INTEL=n
CONFIG_WLAN_VENDOR_INTERSIL=n
CONFIG_WLAN_VENDOR_MARVELL=n
CONFIG_WLAN_VENDOR_MEDIATEK=n
CONFIG_WLAN_VENDOR_RALINK=n
CONFIG_WLAN_VENDOR_REALTEK=n
CONFIG_WLAN_VENDOR_RSI=n
CONFIG_WLAN_VENDOR_ST=n
CONFIG_WLAN_VENDOR_TI=n
CONFIG_WLAN_VENDOR_ZYDAS=n
CONFIG_WLAN_VENDOR_QUANTENNA=n
